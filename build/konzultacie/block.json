{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/konzultacie",
  "version": "0.1.0",
  "title": "Konzultácie",
  "category": "design",
  "icon": "info",
  "description": "Blok pre zobrazenie konzultačných hodín.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Konzultačné hodiny"
    },
    "description": {
      "type": "string",
      "default": "Zaujíma vás naša činnosť? Chcete sa niečo poradiť? Navštívte nás počas konzultačných hodín."
    },
    "field1Icon": {
      "type": "string",
      "default": "clock"
    },
    "field1Bold": {
      "type": "string",
      "default": "Konzultačné hodiny:"
    },
    "field1Text": {
      "type": "string",
      "default": "Pondelok – Piatok: 9:00 – 15:00"
    },
    "field2Icon": {
      "type": "string",
      "default": "location"
    },
    "field2Bold": {
      "type": "string",
      "default": "Adresa:"
    },
    "field2Text": {
      "type": "string",
      "default": "Ulica 123, 000 00 Mesto"
    },
    "field3Icon": {
      "type": "string",
      "default": "phone"
    },
    "field3Bold": {
      "type": "string",
      "default": "Kontakt:"
    },
    "field3Text": {
      "type": "string",
      "default": "+421 000 000 000"
    },
    "buttonText": {
      "type": "string",
      "default": "Kontaktovať nás"
    },
    "buttonUrl": {
      "type": "string",
      "default": "#"
    },
    "buttonNewTab": {
      "type": "boolean",
      "default": false
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "textColor": {
      "type": "string",
      "default": "#333333"
    },
    "accentColor": {
      "type": "string",
      "default": "#fcb722"
    },
    "buttonColor": {
      "type": "string",
      "default": "#fcb722"
    },
    "buttonTextColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "imageId": {
      "type": "number"
    },
    "imageUrl": {
      "type": "string"
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "slider",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}